{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8090",
    "dev:simple": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:secure": "npm run security:cleanup && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:hooks": "jest --testPathPattern=hooks",
    "test:integration": "jest --testPathPattern=integration",
    "test:ci": "jest --coverage --watchAll=false --maxWorkers=2",
    "test:e2e": "playwright test --config=e2e/config/playwright.config.ts",
    "test:e2e:headed": "playwright test --config=e2e/config/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=e2e/config/playwright.config.ts --debug",
    "test:e2e:public": "playwright test --config=e2e/config/playwright.config.ts e2e/tests/public-routes.spec.ts",
    "test:e2e:admin": "playwright test --config=e2e/config/playwright.config.ts e2e/tests/admin-routes.spec.ts",
    "test:e2e:guest": "playwright test --config=e2e/config/playwright.config.ts e2e/tests/guest-routes.spec.ts",
    "test:e2e:auth": "playwright test --config=e2e/config/playwright.config.ts e2e/tests/auth-routes.spec.ts",
    "test:e2e:flows": "playwright test --config=e2e/config/playwright.config.ts e2e/tests/user-flows.spec.ts",
    "test:e2e:report": "playwright show-report artefacts/e2e_dataflow/report",
    "test:e2e:install": "playwright install",
    "test:all": "npm run test:ci && npm run test:e2e",
    "auto-patch": "node scripts/auto-patch/index.js",
    "auto-patch:watch": "node scripts/auto-patch/index.js watch",
    "auto-patch:cleanup": "node scripts/auto-patch/index.js cleanup",
    "auto-patch:demo": "node scripts/auto-patch/demo.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:cleanup": "echo 'Security cleanup skipped'",
    "security:scan": "./scans/security/security-scan.sh",
    "performance:scan": "./scans/performance/performance-scan.sh",
    "bundle:analyze": "node scans/performance/bundle-analyzer.js",
    "lighthouse:audit": "npx lhci autorun --config=scans/performance/lighthouse-config.js",
    "scan:all": "./scans/run-all-scans.sh",
    "scan:security": "npm run security:scan",
    "scan:performance": "npm run performance:scan && npm run bundle:analyze"
  },
